<div class="container mx-auto flex flex-col border">


    <div class="flex bg-white px-6 py-12 gap-6 border">
        <div class="h-32 w-32 rounded-full overflow-hidden border">
            <img src="<%= user.profile_image %>" alt="" class="w-full h-full object-cover">
        </div>
        <div class="flex flex-col justify-center gap-2">
            <h1 class="text-4xl"><%= user.user_name %></h1>
            <h2 class="text-2xl"><%= user.user_firstname + " " + user.user_lastname%></h2>
        </div>
    </div>

</div>

<script src="/js/scripts.js"></script>